
E3_MODULES_VARIABLES:=$(sort $(filter-out $(VARS_EXCLUDES) VARS_EXCLUDES,$(.VARIABLES)))

.PHONY : vars env header


vars: header
        $(foreach v, $(E3_MODULES_VARIABLES), $(info $(v) = $($(v)))) @#noop
         
env: vars

header:
        @echo ""
        @echo "-------------------------------------------------"
        @echo "   Current EPICS and E3 Envrionment Variables "
        @echo "-------------------------------------------------"
